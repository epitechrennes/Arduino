                pass

    def onInput_onStart(self, p):
        strDirName = altools.getDirectoryName( p );
        p = p[len(strDirName):];
        altools.playSound( p );
        self.onStopped() # activate output of the box

    def onInput_onStop(self):
        self.onUnload()]]>
